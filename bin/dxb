#!/usr/bin/env python
# -*- encoding: utf-8 -*-

import sys

if __name__ == '__main__':
    arg = sys.argv[1]
    if arg.startswith('0x'):
        val = int(arg, 16)
    elif arg.startswith('0b'):
        val = int(arg, 2)
    else:
        val = int(arg)

    decf = str(val)
    hexf = hex(val)
    binf = bin(val)

    print '%s %s %s' % (decf, hexf, binf)
